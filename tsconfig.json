    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
    "include": ["src/**/*", "declarations.d.ts"],
    "exclude": ["node_modules", "**/*.spec.ts"]
